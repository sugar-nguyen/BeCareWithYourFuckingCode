@model IEnumerable<BeCareWithYourFuckingCode.Models.TB_BILL>

<div class="nav-fill">
    <h4 class="text-center mt-lg-2 black-text">THÔNG TIN TÀI KHOẢN</h4>
    <hr />
    <table class="table m-auto p-0">
        <thead class="white black-text">
            <tr>

     
                <th scope="col">Tên đăng nhập</th>
                <th scope="col">Giá đề nghị</th>
                <th scope="col">Giá mua</th>
                <th scope="col">Ngày mua</th>
            </tr>
        </thead>
        <tbody>
           @foreach (var i in Model) { 
               <tr>
                   <td>
                       @i.GAME_ACCOUNT_ID
                   </td>
                   <td>
                       @i.OFFER_PRICE
                   </td>
                   <td>
                       @i.SUCCESS_PRICE
                   </td>
                   <td>
                       @i.BILL_DATE
                   </td>
                   
               </tr>
           }
        </tbody>
    </table>
</div>
